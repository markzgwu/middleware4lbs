<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="Buffer1" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer2" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer3" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer100" maxElementsInMemory="100"
		maxElementsOnDisk="100" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer200" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer300" maxElementsInMemory="300"
		maxElementsOnDisk="300" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer400" maxElementsInMemory="400"
		maxElementsOnDisk="400" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="Buffer500" maxElementsInMemory="500"
		maxElementsOnDisk="500" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="BufferFIFO" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />

	<cache name="BufferLRU" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="BufferLFU" maxElementsInMemory="200"
		maxElementsOnDisk="200" eternal="false" timeToIdleSeconds="1440"
		diskPersistent="false" timeToLiveSeconds="2880" overflowToDisk="false"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

</ehcache>